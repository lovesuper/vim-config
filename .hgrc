[ui]
username =
verbose = True
;ignore = 

[auth]
wbst.prefix = 
wbst.username = 
wbst.password =
wbst.schemes = http https

[hostfingerprints]
hg.webesludio.ue = 

[web]
push_ssl = false
allow_push = *

[hooks]

[extensions]
graphlog =
pager =
shelve =
color =
progress =
rebase =
purge =
histedit =
mq =

[alias]
show = log --color=always -pr
nudge = push --rev .

[pager]
pager = LESS='FSRX' less
ignore = version, help, update, serve, record, crecord
attend = cat, diff, glog, log, incoming, outgoing, show, sglog, slog, nlog

[diff]
git = True

[defaults]
addremove = --similarity 100

[schemes]
w = https://

